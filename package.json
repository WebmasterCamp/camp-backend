{
  "name": "ywc15-api",
  "version": "1.0.0",
  "description": "RESTFul API Server for YWC#15 Website. Code reuse from YWC#14",
  "main": "build/bin/server.js",
  "scripts": {
    "eslint": "eslint --quiet **/**/*.js **/*.js **.js",
    "dev": "babel-node ./src/bin/server",
    "dev-cron": "babel-node ./src/cron.js",
    "build": "babel src --out-dir build --source-maps",
    "build:file": "babel src --out-file build/bundle.js --source-maps",
    "build:clear": "rm -rf build",
    "start": "pm2 start pm2_deploy.json --env production",
    "start-staging": "pm2 start pm2_deploy.json --env staging",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pm2-dev": "pm2 delete all; pm2-dev start pm2.json",
    "pm2:start": "pm2 start pm2_deploy.json --env production",
    "newclear": "rm -rf node_module && npm i",
    "genAdmin": "babel-node ./uploads/genAdmin.js",
    "getAnswer": "babel-node ./script/get_answers.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/komcal/ywc-register-backend-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/komcal/ywc-register-backend-api/issues"
  },
  "homepage": "https://github.com/komcal/ywc-register-backend-api#readme",
  "dependencies": {
    "@slack/client": "^3.10.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.15.2",
    "cloudinary": "^1.4.1",
    "compression": "^1.6.2",
    "config": "^1.21.0",
    "connect-mongo": "^1.3.2",
    "cors": "^2.7.1",
    "cron": "^1.2.1",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-flash": "0.0.2",
    "express-jwt": "^5.1.0",
    "express-session": "^1.14.0",
    "express-validator": "^2.20.8",
    "fb": "^2.0.0",
    "html-pdf": "^2.2.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.15.0",
    "lusca": "^1.4.1",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.2",
    "mongoose": "^4.5.9",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "papaparse": "^4.3.6",
    "passport": "^0.3.2",
    "passport-facebook-token": "^3.3.0",
    "randomstring": "^1.1.5",
    "socket.io": "^2.0.4",
    "superagent": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "pm2": "^2.5.0"
  }
}
